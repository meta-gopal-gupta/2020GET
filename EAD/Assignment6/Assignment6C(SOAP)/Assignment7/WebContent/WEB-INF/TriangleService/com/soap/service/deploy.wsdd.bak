<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TriangleService WSDL service -->

  <service name="Triangle" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.soap.com"/>
      <parameter name="wsdlServiceElement" value="TriangleService"/>
      <parameter name="schemaQualified" value="http://service.soap.com"/>
      <parameter name="wsdlServicePort" value="Triangle"/>
      <parameter name="className" value="com.soap.service.TriangleSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Triangle"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getArea" qname="operNS:getArea" xmlns:operNS="http://service.soap.com" returnQName="retNS:getAreaReturn" xmlns:retNS="http://service.soap.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:side1" xmlns:pns="http://service.soap.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:side2" xmlns:pns="http://service.soap.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:side3" xmlns:pns="http://service.soap.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getArea"/>

  </service>
</deployment>
